# Use wdisplays to set up displays once, then
# use swaymsg -g get_outputs

output "Ancor Communications Inc BE249 K8LMQS150300" alias $schreibtisch-links
output "Lenovo Group Limited T24i-2L VNA7WF21" alias $schreibtisch-rechts

output eDP-1 scale 1.5

profile laptop {
    output eDP-1
}


profile desk {
    output $schreibtisch-links mode 1920x1080 position 0,0
    output $schreibtisch-rechts mode 1920x1080 position 1920,0
    output eDP-1 disable
}
